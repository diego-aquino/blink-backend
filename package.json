{
  "name": "blink",
  "version": "0.0.0",
  "description": "URL shortener",
  "license": "MIT",
  "packageManager": "npm@10.7.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "author": "Diego Aquino <diegocruzdeaquino@gmail.com>",
  "scripts": {
    "dev": "dotenv -c development -- tsx watch --clear-screen=false src/index.ts",
    "build": "tsc",
    "start": "dotenv -c production -- node dist/index.js",
    "style:check": "prettier --check",
    "style:format": "prettier --write",
    "types:check": "tsc --noEmit",
    "test": "dotenv -c test -- dotenv -c development -- vitest",
    "typegen": "zimic typegen openapi ./docs/spec/openapi.yaml -o ./src/types/generated.ts --service-name Blink",
    "migration:create": "dotenv -c development -- prisma migrate dev --create-only",
    "migration:apply": "dotenv -c development -- prisma migrate deploy",
    "migration:resolve": "dotenv -c development -- prisma migrate resolve",
    "db:studio": "dotenv -c development -- prisma studio",
    "db:drop-test": "dotenv -c test -- dotenv -c development -- tsx ./scripts/database/dropTestSchemas.ts",
    "deps:up": "docker compose up -d --wait blink-database",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.17.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^5.0.0",
    "jose": "^5.6.3",
    "swagger-ui-dist": "^5.17.14",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.11",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-dist": "^3.30.5",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "execa": "^9.3.1",
    "prettier": "^3.3.3",
    "prettier-plugin-sh": "^0.14.0",
    "prettier-plugin-sql": "^0.18.1",
    "prisma": "^5.17.0",
    "supertest": "^7.0.0",
    "tsx": "^4.16.3",
    "typescript": "^5.5.3",
    "vitest": "^2.0.5",
    "zimic": "^0.9.2-canary.7"
  }
}
