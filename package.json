{
  "name": "blink",
  "version": "0.0.0",
  "description": "URL shortener",
  "license": "MIT",
  "packageManager": "npm@10.7.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "author": "Diego Aquino <diegocruzdeaquino@gmail.com>",
  "scripts": {
    "dev": "dotenv -c development -- tsx watch src/server.ts",
    "build": "tsc",
    "start": "dotenv -c production -- node dist/server.js",
    "style:check": "prettier --check",
    "style:format": "prettier --write",
    "types:check": "tsc --noEmit",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.17.0",
    "express": "^4.19.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/gulp": "^4.0.17",
    "@types/node": "^20.14.11",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "gulp": "^5.0.0",
    "gulp-cli": "^3.0.0",
    "gulp-nodemon": "^2.5.0",
    "gulp-tslint": "^8.1.4",
    "gulp-typescript": "^6.0.0-alpha.1",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "tsx": "^4.16.3",
    "typescript": "^5.5.3"
  }
}
