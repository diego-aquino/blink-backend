{
  "name": "blink",
  "version": "0.0.0",
  "description": "URL shortener",
  "license": "MIT",
  "packageManager": "npm@10.7.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "author": "Diego Aquino <diegocruzdeaquino@gmail.com>",
  "scripts": {
    "dev": "dotenv -c development -- tsx watch src/server.ts",
    "build": "dotenv -c production -- tsup",
    "start": "dotenv -c production -- node ./dist/server.js",
    "style:check": "npm run style --check",
    "style:format": "npm run style --write",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.10",
    "dotenv-cli": "^7.4.2",
    "prettier": "^3.3.2",
    "tsup": "^8.1.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  }
}
